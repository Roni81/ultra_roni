   "source": [
    "x = int(input(\"갯수를 넣으시오(1~10)\"))\n",
    "y = int(input(\"원하는 합을 넣으시오(1~100)\"))\n",
    "cen_num = y//x\n",
    "nums = []\n",
    "res = cen_num-(x-1)//2\n",
    "if y % x == 0:\n",
    " for i in range(x):\n",
    "    nums.append(res)\n",
    "    res+=1\n",
    " print(nums)\n",
    "else:\n",
    " print(\"숫자가 나올 수 없습니다\")"
